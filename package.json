{
  "name": "fabnavi-app",
  "version": "0.3.2",
  "description": "fabnavi app",
  "main": "app/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/fabnavi/fabnavi-app.git"
  },
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "start": "electron .",
    "build": "npm-run-all  --parallel build:*",
    "watch": "webpack --progress --colors --watch",
    "watch:main": "cpx \"src/{index.html,main.js,images/*,fonts/*}\" app/ -w",
    "hot": "webpack-dev-server --hot",
    "build:watch": "webpack --progress --colors --watch",
    "build:electron": "electron .",
    "dist": "build --mac",
    "dist:win": "build --win",
    "release": "build --mac --win --publish always",
    "clean": "rimraf app/[^package.json]* dist/*",
    "lint:fix": "eslint -c ./.eslintrc --fix src/**/*.{js,jsx} src/main.js",
    "lint": "eslint -c ./.eslintrc ./src/**/*.{js,jsx} src/main.js"
  },
  "author": {
    "name": "Kazuma Takada",
    "email": "k222.tkd.fms@gmail.com"
  },
  "contributors": [
    {
      "name": "Kentaro Teramoto",
      "email": "hrlclb@gmail.com",
      "url": "https://horol.org"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.23.0",
    "cpx": "^1.5.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.4",
    "electron": "^1.7.3",
    "electron-builder": "^18.5.1",
    "electron-load-devtool": "^0.5.1",
    "eslint": "^4.0.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "spectron": "^3.7.2",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-polyfill": "^6.23.0",
    "bourbon": "^4.3.4",
    "bourbon-neat": "^2.0.0",
    "debug": "^2.6.8",
    "electron-is-dev": "^0.2.0",
    "electron-log": "^2.2.9",
    "electron-updater": "^2.8.9",
    "history": "^4.6.3",
    "keymirror": "^0.1.1",
    "prop-types": "^15.5.10",
    "qs": "^6.5.0",
    "react": "^16.0.0",
    "react-color": "^2.13.8",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "5.0.0-alpha.6",
    "react-simple-dropdown": "^3.2.0",
    "redux": "^3.7.0",
    "redux-actions": "^2.0.3",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.4.0",
    "styled-jsx": "^1.0.9"
  },
  "build": {
    "appId": "com.github.fabnavi.fabnavi-app",
    "productName": "fabnavi",
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": "nsis"
    }
  }
}